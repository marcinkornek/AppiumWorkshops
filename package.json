{
  "name": "appiumworkshop",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "web": "webpack serve --config web/webpack.config.js",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "gen:env": "yarn rnuc .env",
    "postinstall": "patch-package",
    "test-e2e:android": "wdio run ./test/wdio.android.conf.ts",
    "test-e2e:android:build": "cd android && ./gradlew assembleRelease assembleAndroidTest -DtestBuildType=release && cd ..",
    "test-e2e:ios": "wdio run ./test/wdio.ios.conf.ts",
    "test-e2e:ios:build": "xcodebuild -workspace ios/AppiumWorkshop.xcworkspace -scheme AppiumWorkshopRelease -sdk iphonesimulator -derivedDataPath ios/build",
    "test-e2e:web": "wdio run ./test/wdio.web.conf.ts",
    "test-e2e:web:headless": "wdio run ./test/wdio.web.headless.conf.ts"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.3.1",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/native-stack": "^6.6.2",
    "@types/react-native-vector-icons": "^6.4.11",
    "axios": "^0.27.2",
    "jotai": "^1.7.4",
    "react": "18.0.0",
    "react-dom": "^18.2.0",
    "react-native": "0.69.1",
    "react-native-mmkv": "^2.4.3",
    "react-native-safe-area-context": "^4.3.1",
    "react-native-screens": "^3.14.0",
    "react-native-toast-message": "^2.1.5",
    "react-native-ultimate-config": "^4.0.0",
    "react-native-vector-icons": "^9.2.0",
    "react-native-web": "^0.18.6",
    "react-query": "^3.39.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-native": "^0.69.1",
    "@types/react-native-vector-icons": "^6.4.11",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "@wdio/appium-service": "^7.20.3",
    "@wdio/cli": "^7.20.5",
    "@wdio/cucumber-framework": "^7.20.3",
    "@wdio/local-runner": "^7.20.5",
    "@wdio/spec-reporter": "^7.20.3",
    "@wdio/types": "^7.20.3",
    "appium": "^1.22.3",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.5",
    "babel-plugin-react-native-web": "^0.18.6",
    "chromedriver": "^103.0.0",
    "css-loader": "^6.7.1",
    "eslint": "^7.32.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.70.3",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "react-test-renderer": "18.0.0",
    "style-loader": "^3.3.1",
    "ts-node": "^10.8.2",
    "typescript": "^4.4.4",
    "wdio-chromedriver-service": "^7.3.2",
    "webdriverio": "^7.20.5",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  },
  "resolutions": {
    "@types/react": "^18"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
